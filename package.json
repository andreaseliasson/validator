{
  "name": "@relekang/validator",
  "version": "0.1.2",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib",
    "test": "ava || true",
    "semantic-release": "false && semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relekang/validator.git"
  },
  "author": "Rolf Erik Lekang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relekang/validator/issues"
  },
  "homepage": "https://github.com/relekang/validator#readme",
  "ava": {
    "verbose": true,
    "require": [
      "babel-core/register"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "plugins": [
      "ava"
    ],
    "rules": {
      "ava/test-ended": 2,
      "semi": [
        2,
        "never"
      ]
    }
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-ava": "0.0.1",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "lodash.assign": "^4.0.0",
    "lodash.forown": "^4.0.0",
    "lodash.includes": "^4.0.0"
  }
